{
  "manifest_version": 3,
  "name": "Site Data Cleaner",
  "version": "1.0",
  "description": "Clear site data with one click",
  "permissions": [
    "activeTab",
    "browsingData"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
} 